<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
 
    <xsd:complexType name="customer-address-type">
 		<xsd:sequence>
			<xsd:element name="address" type="xsd:string"></xsd:element>
			<xsd:element name="addresscategory" type="xsd:string" nillable="true"></xsd:element>
			<xsd:element name="cif-id" type="xsd:string"></xsd:element>
			<xsd:element name="city" type="xsd:string" nillable="true"></xsd:element>
			<xsd:element name="country" type="xsd:string" nillable="true"></xsd:element>
			<xsd:element name="state" type="xsd:string"></xsd:element>
			<xsd:element name="zip" type="xsd:string"></xsd:element>
		</xsd:sequence>
    </xsd:complexType>
    		
    <xsd:complexType name="cutomer-addresses-type">
    	<xsd:sequence minOccurs="0" maxOccurs="unbounded">
			<xsd:element ref="customer-address"></xsd:element>
    	</xsd:sequence>
    </xsd:complexType>

    <xsd:element name="customer-addresses" type="cutomer-addresses-type"></xsd:element>
    <xsd:element name="customer-address" type="customer-address-type"></xsd:element>
</xsd:schema>